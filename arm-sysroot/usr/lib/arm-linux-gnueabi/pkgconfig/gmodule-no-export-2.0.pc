prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/arm-linux-gnueabi
includedir=${prefix}/include

gmodule_supported=true

Name: GModule
Description: Dynamic module loader for GLib
Requires: glib-2.0
Version: 2.32.1
Libs: -L${libdir} -lgmodule-2.0 -pthread -lrt
Libs.private: -ldl
Cflags: -pthread
